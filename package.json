{
  "name": "url-shortener-service",
  "version": "1.0.0",
  "description": "Url shortening service",
  "main": "index.js",
  "scripts": {
    "docker": "docker-compose up",
    "dev": "npm run migrate:dev && nodemon src/index.ts",
    "local": "node -r ts-node/register --inspect=0.0.0.0:9231 src/index.ts",
    "migrate:dev": "sequelize-cli db:migrate",
    "build": "tsc --project tsconfig.build.json --verbose",
    "prod": "docker-compose -f docker-compose.yaml -f docker-compose.prod.yaml up",
    "test:unit": "jest --runInBand --testMatch '**/src/**/?(*.)+(spec|test).ts' --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrinorl/url-shortener-service.git"
  },
  "author": "rrinorl@users.noreply.github.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rrinorl/url-shortener-service/issues"
  },
  "homepage": "https://github.com/rrinorl/url-shortener-service#readme",
  "dependencies": {
    "ajv": "8.8.2",
    "ajv-formats": "^2.1.1",
    "better-ajv-errors": "^1.2.0",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-openapi-validator": "4.13.8",
    "http-status-codes": "^2.2.0",
    "is-base64": "^1.1.0",
    "morgan": "1.10.0",
    "nanoid": "^4.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "6.21.1",
    "swagger-ui-express": "4.1.6",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/morgan": "1.9.3",
    "eslint-plugin-jest": "24.3.5",
    "jest": "26.6.3",
    "nodemon": "2.0.18",
    "sequelize-cli": "6.4.1",
    "ts-jest": "26.5.6",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16 <17"
  }
}
