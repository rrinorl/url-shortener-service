{
  "name": "url-shortener-service",
  "version": "1.0.0",
  "description": "Url shortening service",
  "homepage": "https://github.com/rrinorl/url-shortener-service#readme",
  "bugs": {
    "url": "https://github.com/rrinorl/url-shortener-service/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrinorl/url-shortener-service.git"
  },
  "license": "MIT",
  "author": "rrinorl@users.noreply.github.com",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json --verbose",
    "dev": "nodemon src/index.ts",
    "docker": "docker-compose up",
    "format": "prettier --write '**/(*.ts)'",
    "lint": "eslint --fix --no-error-on-unmatched-pattern index.ts package.json '{src/,__tests__/}/**' --ext .ts,.json",
    "lint:report": "yarn run lint -f json -o reports/linting/eslint.json",
    "local": "node -r ts-node/register --inspect=0.0.0.0:9231 src/index.ts",
    "prisma:generate": "prisma generate",
    "prod": "docker-compose -f docker-compose.yaml -f docker-compose.prod.yaml up",
    "test:unit": "jest --runInBand --testMatch '**/src/**/?(*.)+(spec|test).ts' --coverage"
  },
  "dependencies": {
    "@prisma/client": "4.0.0",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "http-status-codes": "2.2.0",
    "morgan": "1.10.0",
    "nanoid": "3.3.4",
    "prisma": "4.0.0"
  },
  "devDependencies": {
    "@jest-mock/express": "2.0.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/morgan": "1.9.3",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-prettier": "4.1.0",
    "jest": "28.1.1",
    "nodemon": "2.0.18",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16 <17"
  }
}
